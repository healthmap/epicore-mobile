<?xml version='1.0' encoding='utf-8'?>
<widget id="org.healthmap.epicore" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Epicore</name>
    <description>
	Epicore web app for mobile devices.
    </description>
    <author email="jeff.andre@childrens.harvard.edu" href="www.epicore.org">
        Epicore Team
    </author>
    <content src="index.html" />
    <plugin name="cordova-plugin-whitelist" spec="1" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
    <engine name="android" spec="~6.1.2" />
    <plugin name="phonegap-plugin-push" spec="~1.10.0">
        <variable name="SENDER_ID" value="808458117906" />
    </plugin>
    <exclude-files pattern="**/scripts" />
    <exclude-files pattern="**/.git" />
    <exclude-files pattern=".gitignore" />
    <exclude-files pattern="**/.idea" />
    <engine name="browser" spec="~4.1.0" />
    <plugin name="cordova-plugin-device" spec="~1.1.5" />
    <engine name="ios" spec="~4.3.1" />
    <plugin name="cordova-plugin-exclude-files" spec="~0.2.1" />
</widget>
